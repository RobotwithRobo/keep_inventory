{
  "type": "minecraft:multi_action",
  "title": "Keep Inventory",
  "body": {
    "type": "minecraft:plain_message",
    "contents": ""
  },
  "inputs": [
    {
      "type": "minecraft:text",
      "key": "number",
      "label": "              Number you want to buy"
    }
  ],
  "exit_action": {
    "label": "Leave",
    "action": {
      "type": "minecraft:run_command",
      "command": "/tellraw @s [{\"text\":\"Number of keep inventory uses left: \", \"color\":\"aqua\"},{\"score\":{\"name\":\"keep_inventory\",\"objective\":\"keep_inventory\"}}]"
    }
  },
  "actions": [
    {
      "label": "Add the number above",
      "tooltip": "5 Emeralds Each",
      "action": {
        "type": "dynamic/run_command",
        "template": "scoreboard players add @s keep_inventory $(number)"
      }
    }
  ]
}